{
  @client({name:"default",kind:"echo"})
  test1: { @describe("@every") 
    req: {
      v1: "integer(1, 10)", @mock
    },
    res: {
      v1: [ @every
        "$ > -1", @eval
        "$ > 0", @eval
      ]
    }
  },
  test2: { @describe("fail# @every")
    req: {
      v1: "integer(1, 10)", @mock
    },
    res: {
      v1: [ @every
        "$ > 10", @eval
        "$ > 0", @eval
        "$ > 20", @eval
      ]
    }
  },
  test3: { @describe("fail# @every must be array value")
    req: {
      v1: "integer(1, 10)", @mock
    },
    res: {
      v1: { @every
      }
    }
  },
}
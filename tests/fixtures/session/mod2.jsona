{
  test1: { @client("echo")
    req: {
      v1: "a"
    }
  },
  group1: { @group
    test1: { @client("echo")
      req: {
        v1: "b"
      }
    },
    test2: { @client("echo")
      req: {
        v1: "c"
      }
    },
    test3: { @client("echo") @describe("access variables")
      req: {

        v4: "mod2.group1.test2.req.v1", @eval
        v5: "group1.test2.req.v1", @eval
        v6: "test2.req.v1", @eval
        v7: "req.v1", @eval
        v8: "test1.req.v1", @eval
      },
      res: {
        v1: "a",
        v2: "b",
        v3: "a",
        v4: "c",
        v5: "c",
        v6: "c",
        v7: "a",
        v8: "a"
      }
    }
  },
  mod2: { @group
    test1: { @client("echo") @describe("no override parent")
      req: {
        v1: "mod2.group1.test2.req.v1", @eval
      },
    }
  },
  env1: { @group
    test1: { @client("echo")  @describe("env variables")
      req: {
        v1: "env.FOO", @eval
      },
      res: {
        v1: "bar"
      }
    }
  }

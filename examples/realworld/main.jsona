{
  @client({
    name: "default",
    kind: "http",
    options: {
      baseURL: "https://conduit.productionready.io/api",
      timeout: 30000
    }
  })
  @mixin("mixin")
  @jslib("@rand")
  @module("auth")
  @module("article1")
  @module("article2")
  @module("profile")
  @module("tag")

  variables: { @client("echo")
    req: {
      username: `"u" + rand.str(8, 3)`, @eval
      email: 'req.username + "@gmail.com"', @eval
      password: `"p" + rand.str(10)`, @eval
    }
  }
}
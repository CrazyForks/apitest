    "prepublish": "npm run -s clean && npm run -s build"
  },
  "dependencies": {
    "@sigodenjs/fake": "^0.2.0",
    "@types/lodash": "^4.14.170",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "jsona-js": "^0.5.1",
    "lodash": "^4.17.21",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "jest": "^27.0.4",
    "pkg": "^5.2.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "pkg": {
    "scripts": "dist/**/*",
    "outputPath": "release"
  }

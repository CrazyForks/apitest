{
  "name": "@sigodenjs/apitest",
  "description": "declartive api testing tool",
  "version": "0.5.0",
  "bin": {
    "apitest": "dist/bin.js"
  },
  "keywords": [
    "api",
    "testing",
    "apitest",
    "ci"
  ],
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/sigoden/apitest",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sigoden/apitest.git"
  },
  "files": [
    "dist",
    "jslib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": "https://github.com/sigoden/apitest/issues",
  "author": "Sigoden Huang <sigoden@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext js --ext ts",
